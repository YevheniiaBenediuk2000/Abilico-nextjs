  "sortingAlgorithmNotes": {
    "description": "Notes on how 'best for me' sorting should work",
    "factors": [
      {
        "factor": "Keyword Match Score",
        "weight": 0.4,
        "description": "Percentage of reviews mentioning positive accessibility features that match user preferences. Higher percentage = higher score."
      },
      {
        "factor": "Category Ratings Average",
        "weight": 0.4,
        "description": "Average rating across user's preferred accessibility categories. Uses category_ratings from reviews."
      },
      {
        "factor": "Review Count",
        "weight": 0.1,
        "description": "More reviews = more reliable data. Normalized score based on review count (capped at reasonable max)."
      },
      {
        "factor": "Overall Rating",
        "weight": 0.1,
        "description": "General quality indicator. Places with higher overall ratings are preferred."
      }
    ],
    "negativeKeywords": {
      "description": "Negative keywords (like 'no accessible toilet', 'broken elevator') should reduce the score",
      "penalty": 0.2,
      "examples": [
        "no accessible toilet",
        "no accessible parking",
        "broken elevator",
        "stairs at entrance",
        "narrow doorway"
      ]
    }
